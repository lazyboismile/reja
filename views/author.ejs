    <div class="frame">
        <div class="main_frame">
            <div class="left_frame">
                <div class="left-1">
                    <div class="left_1-1">
                        <img 
                            src="images/profile.png"
                            width="156px"
                            height="142px"
                            alt=""
                        >
                    </div>
                    <div class="left_1-2"><%=user.name%></div>
                    <div class="left_1-3"><%=user.profession%></div>
                    <div class="left-icons">
                        <a href="" class="link_href">
                            <div class="icon">
                                <img src="icon/Facebook.svg" class="icon-1" />
                            </div>
                        </a>
                        <a href="" class="link_href">
                            <div class="icon">
                                <img src="icon/Instagram.svg" class="icon-1" />
                            </div>
                        </a>
                        <a href="" class="link_href">
                            <div class="icon">
                                <img src="icon/Twitter.svg" class="icon-1" />
                            </div>
                        </a>
                        <a href="" class="link_href">
                            <div class="icon">
                                <img src="icon/Linkeding.svg" class="icon-1" />
                            </div>
                        </a>
                        <a href="" class="link_href">
                            <div class="icon">
                                <img src="icon/Youtube.svg" class="icon-1" />
                            </div>
                        </a>
                        <a href="" class="link_href">
                            <div class="icon">
                                <img src="icon/Dribbble.svg" class="icon-1" />
                            </div>
                        </a>
                    </div>
                </div>
                <div class="marginer"></div>
                <div class="left-2">
                    <div class="about">
                        <p class="about_icon">Age:</p>
                        <p><%=user.age%></p>
                    </div>
                    <div class="about">
                        <p class="about_icon">Residence:</p>
                        <p><%=user.residence%></p>
                    </div>
                    <div class="about">
                        <p class="about_icon">Freelance:</p>
                        <p class="green_btn">Available</p>
                    </div>
                    <div class="about">
                        <p class="about_icon">Address:</p>
                        <p><%=user.address%></p>
                    </div>
                </div>
                <div class="marginer"></div>
                <div class="left-3">
                    <div class="languages">
                        <h4 class="languages_icon">Languages:</h4>
                        <div class="lan-1">
                            <p>Uzbek</p>
                            <p class="per">100%</p>
                        </div>
                        <div class="lan-1">
                            <p>Korean</p>
                            <p class="per">50%</p>
                        </div>
                        <div class="lan-1">
                            <p>English</p>
                            <p class="per">70%</p>
                        </div>
                    </div>
                </div>
                <div class="marginer"></div>
                <div class="left-4">
                    <div class="skills">
                        <h4 class="skills_icon">Skills:</h4>
                        <div class="skill-1">
                            <p>Html</p>
                            <p class="per">100%</p>
                        </div>
                        <div class="skill-1">
                            <p>CSS</p>
                            <p class="per">50%</p>
                        </div>
                        <div class="skill-1">
                            <p>Js</p>
                            <p class="per">80%</p>
                        </div>
                        <div class="skill-1">
                            <p>NodeJS</p>
                            <p class="per">70%</p>
                        </div>
                        <div class="skill-1">
                            <p>Python</p>
                            <p class="per">90%</p>
                        </div>
                    </div>
                </div>
                <div class="marginer"></div>
                <div class="left-5">
                    <div class="ex_skills">
                        <h4 class="ex_skills_icon">Extra Skills:</h4>
                        <% user.extra_skills.slice(0, 3).forEach(skill => { %>
                            <div class="ex_skill-1">
                                <svg xmlns="http://www.w3.org/2000/svg" width="15" height="15" viewBox="0 0 15 15" fill="none">
                                    <rect x="0.5" y="0.5" width="9" height="9" stroke="#FFB400"/>
                                    <rect x="5.5" y="5.5" width="9" height="9" stroke="#FFB400"/>
                                </svg>
                                <p><%= skill %></p>
                            </div>
                        <% }) %>
                    </div>
                </div>
                
                <div class="marginer">

                </div>
            </div>
            <div class="midd_frame">
